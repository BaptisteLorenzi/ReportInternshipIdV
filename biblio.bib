  pages      = {1--24},
  file       = {Full Text PDF:/home/baptistel/snap/zotero-snap/common/Zotero/storage/T8U3F9YI/Diamond - 2017 - Inhibitory Interneurons in the Retina Types, Circ.pdf:application/pdf}
}


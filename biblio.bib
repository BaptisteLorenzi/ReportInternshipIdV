  pages    = {934--941},
  file     = {Accepted Version:/home/baptistel/snap/zotero-snap/common/Zotero/storage/WCVAYZAB/Nikolaev et al. - 2013 - Synaptic mechanisms of adaptation and sensitizatio.pdf:application/pdf}

  year       = {2019},
  file       = {Full Text PDF:/home/baptistel/snap/zotero-snap/common/Zotero/storage/2R25DGEI/Paszke et al. - 2019 - PyTorch An Imperative Style, High-Performance Dee.pdf:application/pdf}

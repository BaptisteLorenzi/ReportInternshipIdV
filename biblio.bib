  file      = {arXiv Fulltext PDF:/home/baptistel/snap/zotero-snap/common/Zotero/storage/TT9F9ISK/Mahmood et al. - 2018 - Deep Adversarial Training for Multi-Organ Nuclei S.pdf:application/pdf;arXiv.org Snapshot:/home/baptistel/snap/zotero-snap/common/Zotero/storage/XTYHD7DC/1810.html:text/html}
}


  pages    = {963--976.e7},
  file     = {Full Text:/home/baptistel/snap/zotero-snap/common/Zotero/storage/VH62U7TE/Kühn and Gollisch - 2019 - Activity Correlations between Direction-Selective .pdf:application/pdf}
